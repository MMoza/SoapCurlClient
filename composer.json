{
    "name": "tuusuario/soap-client",
    "description": "A custom SOAP client using cURL with logging capabilities for requests and responses.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Angel Moza Barquilla",
            "email": "miguelangelmozabarquilla@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "monolog/monolog": "^2.0",
        "ext-simplexml": "*",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Src\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "test": "phpunit --colors=always"
    },
    "minimum-stability": "stable"
}
